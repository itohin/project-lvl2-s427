{
    "name": "itohin/gendiff",
    "description": "CLI PHP package",
    "type": "project",
    "authors": [
        {
            "name": "Anton Tomin",
            "email": "tohinst@yandex.ru"
        }
    ],
    "bin": [
        "bin/gendiff"
    ],
    "scripts": {
        "phpcs": "phpcs",
        "phpunit": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "Gendiff\\": "src"
        },
        "files": [
            "src/Cli.php",
            "src/Differ.php",
            "src/Parser.php"
        ]
    },
    "require": {
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "*"
    }
}
